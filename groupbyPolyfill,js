// lodash groupby polyfill
let result = {};

function group(name, age) {
    if (result[age]) {
        result[age].push(name)
    } else {
        result[age]=[name]
    }
  
}

const people = [
    { name: "John", age: 30 },
    { name: "Jane", age: 25 },
    { name: "Mike", age: 30 },
    { name: "Sara", age: 25 },
    { name: "Chris", age: 20 }
];
for (let i = 0; i < people.length; i++){

    group(people[i].name,people[i].age)
    
}  

console.log("result",result)
